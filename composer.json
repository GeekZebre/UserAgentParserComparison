{
    "name": "thadafinser/user-agent-parser-matrix",
    "description": "Generate a comparison matrix for different UserAgent parsers",
    "license": "MIT",
    
    "minimum-stability": "dev",
    "prefer-stable": true,
    
    "authors": [
        {
            "name": "ThaDafinser",
            "email": "martin.keckeis1@gmail.com"
        }
    ],
    
    "autoload": {
        "psr-4": {
            "UserAgentParserComparison\\":  "src"
        }
    },
    
    "require": {
        "php": "~7.0",
        
        "thadafinser/user-agent-parser": "dev-master",
        
        "browscap/browscap-php": "dev-master",
        "donatj/phpuseragentparser": "^0.5.0",
        "piwik/device-detector": "^3.6",
        "sinergi/browser-detector": "^6.0",
        "ua-parser/uap-php": ">=3.4.3,<4.0",
        "whichbrowser/parser": ">=2.0.3,<3.0",
        "woothee/woothee": "^1.2",
        "mimmi20/wurfl": "^1.6",
        
        "browscap/browscap": ">=6011,<7000",
        "thadafinser/uap-core": "^0.5",
        "woothee/woothee-testset": "^1.3",
        
        "doctrine/cache": "^1.6",
        "doctrine/orm": "^2.5",
        "ramsey/uuid-doctrine": "^1.0",
        
        "ocramius/package-versions": "~1.0"
    },
    
    "require-dev": {
        "phpunit/PHPUnit": "^5.0",
        
        "fabpot/php-cs-fixer": "^1.11.0"
    }
}
